{
  "expo": {
    "name": "Kiwi",
    "slug": "kiwi",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "kiwi",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#00711e"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kiwi.app",
      "icon": "./assets/iosIcon.png",
      "config": {
        "googleMapsApiKey": "process.env.EXPO_PUBLIC_GOOGLE_MAPS_API_KEY"
      }
    },
    "android": {
      "package": "com.kiwi.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/playstore.png",
        "backgroundColor": "#00711e"
      }
    },
    "web": {},
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": [],
          "enableGooglePay": true
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Kiwi to use your location to find nearby beaches."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6a722c73-7cc6-4251-8df6-d03a46b3c952"
      }
    }
  }
}
